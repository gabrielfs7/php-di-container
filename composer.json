{
  "name": "gabrielfs7/php-di-container",
  "description": "PHP Dependency Injection container based on Json configurations",
  "type" : "project",
  "authors": [
    {
      "name": "Gabriel Felipe Soares",
      "email": "gabrielfs7@gmail.com",
      "role": "Developer"
    }
  ],
  "require" : {
    "php": ">=7.0",
    "psr/container" : "1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "6.3.1",
    "squizlabs/php_codesniffer" : "3.1.0",
    "phpmd/phpmd": "2.6.0",
    "phpbench/phpbench": "dev-master@dev",
    "object-calisthenics/phpcs-calisthenics-rules": "v3.0.1"
  },
  "autoload" : {
    "psr-4" : {
      "GSoares\\DiContainer\\" : "src/DiContainer"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "GSoares\\Test\\DiContainer\\" : "test/DiContainer"
    }
  },
  "config": {
    "bin-dir" : "bin"
  }
}